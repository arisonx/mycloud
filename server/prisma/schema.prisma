generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
  active   Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
